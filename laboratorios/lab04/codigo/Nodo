import java.util.*;
public class Nodo
{
    String autor;
    String tama単o;
    String nombre;
    ArrayList<Nodo> hijos;
    public Nodo(String autor, String tama単o, String nombre){
        this.autor = autor;
        this.tama単o = tama単o;
        this.nombre = nombre;
    }

    public ArrayList descendencia(){
        hijos = new ArrayList<>();
        return hijos;
    }
}
